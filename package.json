  },
  "dependencies": {
    "chalk": "^3.0.0",
    "esm": "^3.2.25",
    "get-stdin": "^7.0.0",
    "iconv-lite": "^0.6.3",
    "indent-string": "^4.0.0",
    "json-colorizer": "^2.2.1",
    "tmp": "^0.2.1",
    "xml-js": "^1.6.11",
    "yaml": "^1.10.0",
    "yargs": "^15.3.1"
